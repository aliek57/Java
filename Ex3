package exercicios_poo;

import java.util.Scanner;

public class Ex3 {

	private static int n1, n2, n3, soma=0;
	
	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		
		n1 = entrada.nextInt();
		n2 = entrada.nextInt();
		n3 = entrada.nextInt();
		
		soma = n1+n2+n3;
		
		System.out.print("soma = " + soma);
	}
}
